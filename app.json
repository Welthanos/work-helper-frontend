{
  "expo": {
    "name": "Work Helper",
    "slug": "work-helper",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/adaptative-logo.png",
    "scheme": "workhelper",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/logo.png",
        "backgroundColor": "#000"
      },
      "edgeToEdgeEnabled": true,
      "package": "work.helper.v1"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0000"
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9a4612ce-5e1f-4760-8e3d-18503e25bdfb"
      }
    }
  }
}